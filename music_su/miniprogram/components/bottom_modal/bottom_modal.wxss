.modal {
  width: 100%;
  height: 100%;
  position: absolute;
  background: rgba(0,0, 0, 0.6);
  z-index: 99;
}

.panel {
  display:flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  width: 100%;
  height: 330rpx;
  background-color: white;
  position: absolute;
  bottom: 0;
  left: 0;
}

.delete-image {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  right: 10rpx;
  top: 10rpx;
  padding: 20rpx;
}
