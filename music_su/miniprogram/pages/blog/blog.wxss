.header {
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  background-color: white;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  z-index: 99;
  box-sizing: border-box;
}
.publish-container {
  width: 64rpx;
  height: 64rpx;
  align-items: center;
  display: flex;
  justify-content: center;
}

.publish-image {
  width: 64rpx;
  height: 64rpx;
}

.search-container {
  flex: 1;
  background-color: teal;
}